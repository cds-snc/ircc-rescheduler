{
  "name": "ircc-rescheduler",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "razzle start",
    "build": "razzle build",
    "test": "node_modules/jest/bin/jest.js",
    "start": "NODE_ENV=production node build/server.js",
    "dbg": "node --inspect-brk node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "@cdssnc/gcui": "^0.0.16",
    "@jaredpalmer/after": "latest",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link-http": "^1.5.3",
    "emotion": "^9.1.1",
    "emotion-server": "^9.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "isomorphic-fetch": "^2.2.1",
    "npm": "^5.8.0",
    "raf": "^3.4.0",
    "razzle": "^0.8.14",
    "react": "^16.3.1",
    "react-apollo": "^2.1.2",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/razzle/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupTests.js",
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.js",
      "<rootDir>/src/**/?(*.)(spec|test).js",
      "<rootDir>/test/**/?(*.)(spec|test).js"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "babel-plugin-emotion": "^9.1.0",
    "jest": "20.0.4",
    "prettier": "^1.11.1",
    "react-emotion": "^9.1.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "supertest": "^3.0.0"
  }
}
