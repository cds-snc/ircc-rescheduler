{
  "name": "ircc-rescheduler",
  "version": "1.0.3",
  "license": "MIT",
  "scripts": {
    "build": "razzle build",
    "dev": "razzle start",
    "dbg": "node --inspect-brk node_modules/jest/bin/jest.js",
    "lint": "node_modules/eslint/bin/eslint.js src test",
    "postinstall": "razzle build",
    "start": "NODE_ENV=production node --icu-data-dir=./node_modules/full-icu build/server.js",
    "build:start": "yarn build && yarn start",
    "test": "node --icu-data-dir=./node_modules/full-icu node_modules/jest/bin/jest.js",
    "extract": "NODE_ENV=development lingui extract",
    "compile": "NODE_ENV=development lingui compile",
    "add_locale": "lingui add-locale",
    "stats": "BUNDLE_CHECK=true yarn build",
    "a11y:test": "start-server-and-test build:start http://localhost:3004 a11y:run",
    "a11y:run": "node a11y.test.js",
    "nojs:test": "start-server-and-test build:start http://localhost:3004 nojs:run",
    "nojs:run": "jest 'NoJS'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --record",
    "ci:dev": "start-server-and-test dev http://localhost:3004 cypress:open",
    "ci:prod": "start-server-and-test build:start http://localhost:3004 cypress:run",
    "sentry_map": "sentry-cli releases -o canadian-digital-service -p ircc-rescheduler files  $RAZZLE_STAGE  upload-sourcemaps ./build/ --rewrite",
    "sm:staging": "COMMIT_HASH=`git rev-parse --short HEAD` && . $(readlink .env.production) && SENTRY_AUTH_TOKEN=$SENTRY_AUTH_TOKEN RAZZLE_STAGE=staging-$COMMIT_HASH yarn sentry_map",
    "sm:production": "COMMIT_HASH=`git rev-parse --short HEAD` && . $(readlink .env.production) && SENTRY_AUTH_TOKEN=$SENTRY_AUTH_TOKEN RAZZLE_STAGE=production-$COMMIT_HASH yarn sentry_map"
  },
  "dependencies": {
    "@cdssnc/gcui": "^0.0.30",
    "@jaredpalmer/after": "^1.3.1",
    "@sentry/cli": "^1.34.0",
    "aws-sdk": "^2.291.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-lingui-react": "^1.0.13",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "date-fns": "^1.29.0",
    "emotion": "^9.2.6",
    "emotion-server": "^9.2.6",
    "express": "^4.16.3",
    "final-form": "^4.9.1",
    "full-icu": "^1.2.1",
    "helmet": "^3.13.0",
    "hoist-non-react-statics": "^3.0.1",
    "inline-css": "^2.4.0",
    "js-cookie": "^2.2.0",
    "lingui-react": "^1.4.1",
    "nodemailer": "^4.6.7",
    "prop-types": "^15.6.2",
    "raven": "^2.6.3",
    "razzle": "^2.4.0",
    "react": "16.4.2",
    "react-day-picker": "^7.1.10",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.6",
    "react-final-form": "^3.6.5",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.0",
    "unfetch": "^3.1.1",
    "validatorjs": "^3.14.2"
  },
  "devDependencies": {
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "babel-plugin-emotion": "^9.2.6",
    "cypress": "^3.0.3",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "lingui-cli": "^1.4.4",
    "pa11y": "^5.0.4",
    "prettier": "^1.14.2",
    "puppeteer": "^1.7.0",
    "raf": "^3.4.0",
    "start-server-and-test": "^1.7.0",
    "supertest": "^3.1.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/razzle/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "globals": {
      "RAZZLE_PAPER_FILE_NUMBER_PATTERN": "[a-zA-Z]{1}"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupTests.js",
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.js",
      "<rootDir>/src/**/?(*.)(spec|test).js",
      "<rootDir>/test/**/?(*.)(spec|test).js"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "lingui": {
    "srcPathIgnorePatterns": [
      "/node_modules/",
      "build"
    ]
  }
}
