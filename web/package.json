{
  "name": "ircc-rescheduler",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "razzle build",
    "dev": "razzle start",
    "dbg": "node --inspect-brk node_modules/jest/bin/jest.js",
    "lint": "node_modules/eslint/bin/eslint.js src test",
    "start": "NODE_ENV=production node build/server.js",
    "test": "node_modules/jest/bin/jest.js",
    "extract": "NODE_ENV=development lingui extract",
    "compile": "NODE_ENV=development lingui compile",
    "add-locale": "lingui add-locale"
  },
  "dependencies": {
    "@cdssnc/gcui": "^0.0.28",
    "@jaredpalmer/after": "^1.3.1",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.3",
    "apollo-link-state": "^0.4.1",
    "emotion": "^9.1.1",
    "emotion-server": "^9.1.1",
    "express": "^4.16.3",
    "final-form": "^4.4.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "isomorphic-fetch": "^2.2.1",
    "lingui-react": "^1.4.1",
    "moment": "2.22.0",
    "raven-js": "^3.25.2",
    "razzle": "^2.0.0-alpha.12",
    "react": "16.3.1",
    "react-apollo": "^2.1.2",
    "react-day-picker": "^7.1.4",
    "react-dom": "16.3.1",
    "react-emotion": "^9.1.1",
    "react-final-form": "^3.1.5",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "unfetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.13.0",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "babel-plugin-emotion": "^9.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-lingui-react": "^1.0.13",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "lingui-cli": "^1.4.4",
    "prettier": "^1.11.1",
    "raf": "^3.4.0",
    "supertest": "^3.0.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/razzle/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupTests.js",
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.js",
      "<rootDir>/src/**/?(*.)(spec|test).js",
      "<rootDir>/test/**/?(*.)(spec|test).js"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "lingui": {
    "srcPathIgnorePatterns": [
      "/node_modules/",
      "build"
    ]
  }
}
